<%= form_for(@post,url: home_index_path,method: "POST") do |f| %>
    <%= f.label :title, "제목" %>
    <%= f.text_field :title %><br>
    <%= f.label :content, "내용" %>
    <%= f.text_area :content %><br>
    <%= f.submit "제출" %>
<% end %>

<%= link_to home_index_path do %>
    <button>홈으로</button>
<% end %>